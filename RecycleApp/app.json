{
  "expo": {
    "name": "RecycleApp",
    "slug": "RecycleApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recycleapp",
      "googleMapsApiKey": "AIzaSyAY2sQ-OFIbcvK4oNswr2cD-_KkM2WDH_E",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.recycleapp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC_za6RubGd0WOUtvi1OJ5i9N2Ckbh3Z9E"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you share them."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d55bcff2-46f1-4f3e-91c4-27b0ed65a15e"
      }
    },
    "owner": "fahricoskun"
  }
}